{
  "name": "admin-dashboard-react",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint:check": "eslint --ignore-path .eslintignore --ext js,jsx",
    "lint:fix": "eslint . --fix",
    "preview": "vite preview",
    "prettier:check": "prettier --ignore-path .gitignore --write \"src/**/*.{js,jsx}\"",
    "prettier:fix": "prettier --write .",
    "lint-prettier": "npm run lint:check && npm run prettier:check",
    "prepare": "husky install",
    "lint-staged": "npx lint-staged"
  },
  "lint-staged": {
    "src/**/*.{js,jsx}": "npm run lint-prettier"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.15",
    "axios": "^1.4.0",
    "localforage": "^1.10.0",
    "match-sorter": "^6.3.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-paginate": "^8.2.0",
    "react-router-dom": "^6.13.0",
    "sort-by": "^1.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.37",
    "@types/react-dom": "^18.0.11",
    "@vitejs/plugin-react": "^4.0.0",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "husky": "^8.0.0",
    "lint-staged": "^13.2.2",
    "postcss": "^8.4.24",
    "prettier": "^2.8.8",
    "tailwindcss": "^3.3.2",
    "vite": "^4.3.9"
  },
  "contributes": {
    "languages": [
      {
        "id": "css",
        "extensions": [
          ".css"
        ]
      }
    ]
  }
}
